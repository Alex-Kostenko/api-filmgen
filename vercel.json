{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "status": 308,
      "headers": { "Location": "https://api-filmgen-pearl.vercel.app" }
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["POST", "GET", "PUT", "DELETE"]
    }
  ]
}
